//Καταστροφέας

#include <iostream>  
using namespace std;  
class Employee  
 {  
   public:  
        Employee() {    
            cout<<"Constructor "<<endl;    
        }    
        ~Employee {    
            cout<<"Destructor "<<endl;    
        }  
};  
int main(void)   
{  
    Employee e1; 
    Employee e2; 
    return 0;  
}  
