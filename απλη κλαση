//Απλή κλάση 

#include <iostream>  
using namespace std;  
class Student {  
   public:  
      int id; //πεδίο      
      string name; //πεδίο
};  
int main() {  
    Student s1; //Δημιουργία αντικειμένου Student   
    s1.id = 201;    
    s1.name = "Dimitris Papadopoulos";   
    cout << s1.id << endl;  
    cout << s1.name << endl;  
    return 0;  
}  
